{
	"manifest_version": 2,
	
	"name": "Libre-ry Book Search",
	"description": "The fastest way to find free e-books.",
	"version": "1.6",
	
	"permissions": [
	  "tabs",
	  "http://gen.lib.rus.ec/*",
	  "http://libgen.org/*"
	],
	
	"browser_action": { 
	  "default_icon": "icon.png",
	  "default_popup": "popup.html"
	},
	
	"icons": {
	  "128": "icon128.png" 
	 }
}
